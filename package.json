{
    "name": "datautils",
    "description": "Some JavaScript data utilities",
    "version": "0.0.3",
    "main": "./main",
    "dependencies": {
        "underscore": "1.5.2"
    },
    "devDependencies": {
        "chai": "*",
        "jshint": "*",
        "mocha": "*"
    },
    "author": "Simon Emms <simon@simonemms.com>",
    "contributors": [{
        "name": "Simon Emms",
        "email": "simon@simonemms.com"
    }],
    "repository": {
        "type": "git",
        "url": "https://github.com/riggerthegeek/datautils-js.git"
    },
    "bugs": {
        "url": "https://github.com/riggerthegeek/datautils-js/issues"
    },
    "licence": "GPL-3.0",
    "scripts": {
        "test": "jshint ./ && mocha test --colors --reporter spec --recursive"
    },
    "jshintConfig": {
        "camelcase": true,
        "curly": true,
        "eqeqeq": true,
        "strict": true,
        "trailing": true,
        "undef": true,
        "unused": true,
        "globals": {
            "module": false,
            "require": false
        }
    }
}
